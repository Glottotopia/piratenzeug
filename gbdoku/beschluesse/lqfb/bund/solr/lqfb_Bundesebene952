    <field name="title">Bundesparteitag gemäß Beschlusslage vorbereiten</field>
    <field name="date">4 Jahrs 5 Monaten 24 Tagen 08:53:50</field>
    <field name="area">Streitfragen zu Abstimmungen</field>
    <field name="regelwerk">Abstimmungsstreitfrage </field>
    <field name="result">award_star_gold_2.png</field>
    <field name="yes">301</field>
    <field name="no">121</field>
