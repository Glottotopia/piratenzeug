    <field name="ID">lqfb_Bundesebene5957</field>
    <field name="title">AG Gesundheitspolitik - Wahlprogrammvorschlag 2</field>
    <field name="date">1 Jahr 10 Monaten 27 Tagen 13:26:03</field>
    <field name="area">Gesundheit und Drogen/Suchtpolitik</field>
