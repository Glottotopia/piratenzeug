    <field name="title">Für eine solidarische und transparente Entwicklungszusammenarbeit</field>
    <field name="date">2 Jahrs 3 Monaten 1 Tag 09:45:14</field>
    <field name="area">Außen, Internationales, Frieden</field>
    <field name="regelwerk">Programmantrag </field>
    <field name="result">award_star_silver_2.png</field>
